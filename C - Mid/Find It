#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int l;
    scanf("%d",&l);
    int arr[1001]={0};
    for (int i=0; i<l; i++){
        int x;
        scanf("%d",&x);
        arr[x]++;
    }
    int y;
    scanf("%d",&y);
    printf("%d",arr[y]);
    
    return 0;
}
