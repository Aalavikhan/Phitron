#include <stdio.h>
#include <string.h>
int main(){
    char arr[1001];
    scanf("%s", &arr);
    int ln= strlen(arr);
    int i=0;
    int j=ln-1;
    int x=0;
    while(i<j){
        if (arr[i] != arr[j]){
            printf("%s","NO");
            x=1;
            break;
        }
        i++;
        j--;
    }
    if (x==0){
        printf("%s","YES");
    }

    return 0;
}
